    function jsonToParam(jsonData) {
        if (!jsonData || typeof jsonData !== 'object') return jsonData;
        const paramArr = [];

        function buildParam(obj, prefix) {
            for (const key in obj) {
                if (Object.prototype.hasOwnProperty.call(obj, key)) {
                    const value = obj[key];
                    const paramName = prefix ? prefix + '.' + key : key;

                    if (typeof value === 'string' || typeof value === 'number') {
                        if (value) {
                            paramArr.push(paramName + '=' + value);
                        }
                    } else if (Array.isArray(value)) {
                        for (let i = 0; i < value.length; i++) {
                            const subValue = value[i];
                            if (subValue) {
                                paramArr.push(paramName + '=' + subValue);
                            }
                        }
                    } else if (typeof value === 'object' && value !== null) {
                        buildParam(value, paramName);
                    }
                }
            }
        }

        buildParam(jsonData, '');

        console.log('변환결과 :');
        console.log(paramArr);
        console.log('변환결과2 :');
        console.log(paramArr.join('&').trim());

        return paramArr.join('&').trim();
    }




